<table class="table table-bordered table-condensed table-overview">
	  <%= table_row("Person_id:",@employment.person_id) %>
	  <%= table_row("Position_id:",@employment.position_id) %>
	  <%= table_row("Store_id:",@employment.store_id) %>
	  <%= table_row("Start_date:",@employment.start_date) %>
	  <%= table_row("End_date:",@employment.end_date) %>
</table>

<div class="form-actions">
  <%= link_to 'Back', employments_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_employment_path(@employment), :class => 'btn' %>
  <%= link_to 'Delete', employment_path(@employment), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
